{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"workspaces": {
		".": {
			"entry": ["**/*.{json,js,ts}"],
			"typedoc": {
				"config": ["typedoc.base.json", "typedoc.json"]
			},
			"vite": false
		},
		"pkgs/js-ast-analyze": {
			"entry": ["src/lib.ts"],
			"project": ["**/*.{json,js,ts}"]
		},
		"pkgs/js-ast-build": {
			"entry": ["src/lib.ts"],
			"project": ["**/*.{json,js,ts}"]
		},
		"pkgs/ts-ast-analyze": {
			"entry": ["src/lib.ts"],
			"project": ["**/*.{json,js,ts}"]
		},
		"pkgs/ts-ast-build": {
			"entry": ["src/lib.ts"],
			"project": ["**/*.{json,js,ts}"]
		},
		"pkgs/svelte-ast-analyze": {
			"entry": ["src/lib.ts"],
			"project": ["**/*.{json,js,ts}"]
		},
		"pkgs/svelte-ast-build": {
			"entry": ["src/lib.ts"],
			"project": ["**/*.{json,js,ts}"]
		},
		"pkgs/svelte-ast-print": {
			"entry": ["src/lib.ts"],
			"project": ["**/*.{json,js,ts}"]
		}
	}
}
