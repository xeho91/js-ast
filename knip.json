{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"workspaces": {
		".": {
			"entry": ["**/*.{json,js,ts}"],
			"typedoc": {
				"config": ["typedoc.base.json", "typedoc.json"]
			},
			"vite": false
		},
		"packages/*": {
			"entry": ["src/*.ts", "!src/_internal/"],
			"project": ["**/*.{json,js,ts}"]
		}
	}
}
